{% extends '@HWIOAuth/layout.html.twig' %}

{% block hwi_oauth_content %}
    {% if error is defined and error %}
        <span>{{ error }}</span>
    {% endif %}
    <a class="btn btn-primary" href="{{ path('hwi_oauth_service_redirect', {'service': 'jira' }) }}">
        <span>Login with Jira</span>
    </a>
{% endblock hwi_oauth_content %}
